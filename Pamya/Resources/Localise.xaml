<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="file">_File</system:String>
        <system:String x:Key="open">_Open</system:String>
        <system:String x:Key="exit">_Exit</system:String>
    
    <system:String x:Key="edit">_Edit</system:String>

    <system:String x:Key="game">_Game</system:String>
        <system:String x:Key="typegame">_Typing Game</system:String>
        <system:String x:Key="mcgame">_Multiple Choice Game</system:String>
        <system:String x:Key="flashcardgame">_Flash Card Game</system:String>

    <system:String x:Key="options">_Options</system:String>
        <system:String x:Key="card">_Card</system:String>
        <system:String x:Key="deck">_Deck</system:String>

</ResourceDictionary>